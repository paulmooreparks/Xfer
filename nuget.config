      <package pattern="System.Text.Encodings.Web" />
      <package pattern="Microsoft.Extensions.FileSystemGlobbing" />
      <package pattern="Microsoft.Data.Sqlite" />
      <package pattern="Microsoft.TestPlatform.AdapterUtilities" />
