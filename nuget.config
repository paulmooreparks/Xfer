      <package pattern="Microsoft.Testing.Extensions.TrxReport.Abstractions" />
      <package pattern="Microsoft.Testing.Platform" />
      <package pattern="Microsoft.TestPlatform.ObjectModel" />
      <package pattern="Microsoft.ApplicationInsights" />
      <package pattern="Microsoft.NET.Test.Sdk" />
      <package pattern="Cliffer" />
      <package pattern="Microsoft.TestPlatform.TestHost" />
      <package pattern="Microsoft.CodeCoverage" />
      <package pattern="Microsoft.Extensions.Configuration" />
      <package pattern="Microsoft.Extensions.Configuration.Abstractions" />
      <package pattern="Microsoft.Extensions.Configuration.Binder" />
      <package pattern="Microsoft.Extensions.Configuration.Json" />
      <package pattern="Microsoft.Extensions.DependencyInjection" />
